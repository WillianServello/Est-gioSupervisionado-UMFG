@using cafeservellocontroler.Models.ViewModels
@model RevendedorViewModel
@{
    ViewData["Title"] = "Detalhes do Revendedor";
}
<table>
    <tr>
        <td>
            <h2>Detalhes do Revendedor</h2>
            <hr />
            <dl class="row">
                <dt class="col-sm-2">
                    Nome
                </dt>
                <dd class="col-sm-10">
                    @Model.Nome
                </dd>
                <dt class="col-sm-2">
                    Email
                </dt>
                <dd class="col-sm-10">
                    @Model.Email
                </dd>
                <dt class="col-sm-2">
                    CNPJ
                </dt>
                <dd class="col-sm-10">
                    @Model.Cnpj
                </dd>
                <dt class="col-sm-2">
                    Telefone
                </dt>
                <dd class="col-sm-10">
                    @Model.Telefone
                </dd>
                <dt class="col-sm-2">
                    Data Cadastro
                </dt>
                <dd class="col-sm-10">
                    @Model.DataCadastro
                </dd>
                <dt class="col-sm-2">
                    Data da ultima Atualização Cadastro
                </dt>
                <dd class="col-sm-10">
                    @Model.DataAtualizacaoCadastro
                </dd>
            </dl>
            <div>
                <a asp-action="Index" class="btn btn-secondary">Voltar</a>
            </div>
        </td>
    </tr>
</table>